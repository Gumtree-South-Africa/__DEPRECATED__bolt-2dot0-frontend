{{> (base "footer") }}

{{#partial "footer"}}
	<div class="footer-links clearfix">
		<div class="logo">
	        <a href="{{this.header.homePageUrl}}">
	        	<img border="0"  src="{{this.header.baseImageUrl}}{{this.locale}}/logo.png" alt="{{__ "header.country.label"}}"/>
	        </a>
		</div>

		<div>
			<h3>{{{__ "footer.getToKnowUs.title"}}}</h3>
			<ul>
				<li><span class='sudo-link' data-o-uri='{{obfuscateUrl "http://pomoc.gumtree.pl/PL/articles/pl/KB_Article/O-nas"}}'>{{{__ "footer.getToKnowUs.about"}}}</span></li>
				<li><span class='sudo-link' data-o-uri='{{obfuscateUrl "http://pomoc.gumtree.pl/PL?lang=pl&l=pl&c=PKB%3AZasady_Gumtree"}}'>{{{__ "footer.getToKnowUs.rules"}}}</span></li>
				<li><a href="http://blog.gumtree.pl/">{{{__ "footer.getToKnowUs.blog"}}}</a></li> 
			</ul>
		</div>

	    <div>
	        <h3>{{{__ "footer.explore.title"}}}</h3>
	        <ul>
	            {{!-- Top Searches --}}
	             {{#if this.footer.topSearchesLandingPageUrl}}
	             	<li><a href="{{{this.footer.topSearchesLandingPageUrl}}}">{{{__ "footer.explore.topSearches" }}}</a></li>
	             {{else}}
				 	<li><a href="#">{{{__ "footer.explore.topSearches" }}}</a></li>
	             {{/if}}
	             <li><a href="http://www.gumtree.pl/pages/zawartosc/">{{{__ "footer.explore.sitemap" }}}</a></li>
	             <li><a href="http://www.gumtree.pl/pages/lokalizacje/">{{{__ "footer.explore.locations" }}}</a></li>
	             <li><a href="http://www.gumtree.pl/pages/ceny-nieruchomosci">{{{__ "footer.explore.propertyPriceChecker"}}}</a></li>
	   
			</ul>
	    </div>
	    
		<div class="rightColumn">
			<h3>{{{__ "footer.legalbits.title"}}}</h3>
			<ul>
				<li><span class='sudo-link' data-o-uri='{{obfuscateUrl "http://pomoc.gumtree.pl/PL/articles/pl/KB_Article/Zasady-korzystania"}}'>{{{__ "footer.legalbits.terms" }}}</span></li>
				<li><span class='sudo-link' data-o-uri="{{{this.footer.obfuscatedPrivacyPolicyURL}}}" data-target="_self">{{{__ "footer.legalbits.privacy"}}}</span></li>
				<li><span class='sudo-link' data-o-uri='{{obfuscateUrl "http://pomoc.gumtree.pl/PL/articles/pl/KB_Article/Cookies"}}'>{{{__ "footer.legalbits.cookie"}}}</span></li>
			</ul>
		</div>
	    
		<div class="rightColumn">
			<h3>{{{__ "footer.tips-help.title"}}}</h3>
			<ul>
	            <li><a href="http://pomoc.gumtree.pl/PL">{{{__ "footer.tips-help.helpContact"}}}</a></li>
	            <li><span class="sudo-link" data-o-uri='{{obfuscateUrl "http://pomoc.gumtree.pl/PL?lang=pl&l=pl&c=PKB%3ASafetyPL"}}'>{{{__ "footer.tips-help.staySafe"}}}</span></li>
	            <li><span class="sudo-link" data-o-uri='{{obfuscateUrl "http://pomoc.gumtree.pl/PL?lang=pl&l=pl&cu=1&fs=ContactUsq=&s="}}'>{{{__ "footer.tips-help.writeToUs"}}}</span></li>
	             <li><span class="sudo-link" data-o-uri='{{obfuscateUrl "http://pomoc.gumtree.pl/PL?lang=pl&l=pl&c=PKB%3ABasicsPL"}}'>{{{__ "footer.tips-help.promotingAds"}}}</span></li>
			</ul>
		</div>
	</div>

	<div class="social-links">{{> (dynamic "socialMediaLinks")}}</div>
	<div class="cpyrt">{{{__ "footer.copyright"}}}</div>
{{/partial}}

