{{> (base "footer")}}

{{#partial "footer"}}
	<div class="footer-links clearfix">
		<div class="logo">
	        <a href="{{this.header.homePageUrl}}">
	        	<img border="0"  src="{{this.header.baseImageUrl}}{{this.locale}}/logo.png" alt="{{__ "header.country.label"}}"/>
	        </a>
		</div>

		<div>
			<h3>{{{__ "footer.getToKnowUs.title"}}}</h3>
			<ul>
				<li><span class='sudo-link' data-o-uri='{{obfuscateUrl "http://help.gumtree.co.za/articles/KB_Article/About-Us?vgroup1=PublicKB&vcategory1=PublicKB%3AGumtree_Basics&vcategory2=About_us"}}'>{{{__ "footer.getToKnowUs.about"}}}</span></li>
				<li><span class='sudo-link' data-o-uri='{{obfuscateUrl "http://help.gumtree.co.za/articles/KB_Article/What-are-the-General-posting-rules/?vgroup1=PublicKB&vcategory1=PublicKB%3AGumtree_policies"}}'>{{{__ "footer.getToKnowUs.rules"}}}</span></li>
				<li><a href="http://blog.gumtree.co.za/">{{{__ "footer.getToKnowUs.blog"}}}</a></li>
				<li><a href="https://protool.gumtree.co.za/forbusiness/index.html">{{{__ "footer.getToKnowUs.forbusiness"}}}</a></li>
			</ul>
		</div>

	    <div>
	        <h3>{{{__ "footer.explore.title"}}}</h3>
	        <ul>
	            {{!-- Old Mobile Site --}}
	            <li><span class='sudo-link mobilesite' data-o-uri='{{obfuscateUrl "http://m.gumtree.co.za" }}'>{{{__ "footer.explore.mobilesite" }}}</span></li>
	            {{!-- Best Ads --}}
	            <li><a href="{{{this.footer.bestAdsLandingPageUrl}}}">{{{__ "footer.explore.bestof" }}}</a></li>
	            {{!-- Top Searches --}}
	            <li><a href="{{{this.footer.topSearchesLandingPageUrl}}}">{{{__ "footer.explore.topSearches" }}}</a></li>
	            <li><a href="http://www.gumtree.co.za/pages/sitemap/">{{{__ "footer.explore.sitemap" }}}</a></li>
	            {{!-- Locations --}}
	            <li><a id="footer-locationmap" href="http://www.gumtree.co.za/pages/locations/">{{{__ "footer.explore.locations" }}}</a></li>
	            {{!-- eCG interlinking Page --}}
	            <li><a href="http://www.gumtree.co.za/pages/ClassifiedsNetwork/">{{{__ "footer.explore.interlink" }}}</a></li>
	            {{!-- Mobile App --}}
	            <li><a href="http://www.gumtree.co.za/pages/mobileapp/">{{{__ "footer.explore.mobileApp" }}}</a></li>
			</ul>
	    </div>
	    
		<div class="rightColumn">
			<h3>{{{__ "footer.legalbits.title"}}}</h3>
			<ul>
				<li><span class='sudo-link' data-o-uri='{{obfuscateUrl "http://help.gumtree.co.za/articles/KB_Article/Gumtree-Terms-of-Use?vgroup1=PublicKB&vcategory1=PublicKB%3AGumtree_policies&vcategory2=General_policies" }}' data-target="_self">{{{__ "footer.legalbits.terms" }}}</span></li>
				<li><span class='sudo-link' data-o-uri='{{obfuscateUrl "http://help.gumtree.co.za/articles/KB_Article/Gumtree-Privacy-Policy?vgroup1=PublicKB&vcategory1=PublicKB%3AGumtree_policies&vcategory2=General_policies"}}' data-target="_self">{{{__ "footer.legalbits.privacy"}}}</span></li>
				<li><span class='sudo-link' data-o-uri="{{{this.footer.obfuscatedCookieRightsURL}}}" data-target="_self">{{{__ "footer.legalbits.cookie"}}}</span></li>
			</ul>
		</div>
	    
		<div class="rightColumn">
			<h3>{{{__ "footer.tips-help.title"}}}</h3>
			<ul>
	            <li><a href="http://help.gumtree.co.za/">{{{__ "footer.tips-help.helpContact"}}}</a></li>
				<li><span class="sudo-link" data-o-uri='{{obfuscateUrl "http://help.gumtree.co.za/pkb_Home?l=en_US&c=PublicKB%3ASafety_advice" }}'>{{{__ "footer.tips-help.staySafe"}}}</span></li>
				<li><span class="sudo-link" data-o-uri='{{obfuscateUrl "https://www.surveymonkey.com/s/7LGVXY8"}}'>{{{__ "footer.tips-help.giveFeedback"}}}</span></li>
			</ul>
		</div>
		
		<div class="rightColumn">
			<h3>{{{__ "footer.motors.title"}}}</h3>
			<ul>
		        <li><a href="http://www.gumtree.co.za/pages/gumtreemotors/gumtree-motors-library-a-c.html">{{{__ "footer.motors.a-c"}}}</a></li>
	            <li><a href="http://www.gumtree.co.za/pages/gumtreemotors/gumtree-motors-library-d-g.html">{{{__ "footer.motors.d-g"}}}</a></li>
	            <li><a href="http://www.gumtree.co.za/pages/gumtreemotors/gumtree-motors-library-h-l.html">{{{__ "footer.motors.h-l"}}}</a></li>
	            <li><a href="http://www.gumtree.co.za/pages/gumtreemotors/gumtree-motors-library-m-o.html">{{{__ "footer.motors.m-o"}}}</a></li>
	            <li><a href="http://www.gumtree.co.za/pages/gumtreemotors/gumtree-motors-library-p-z.html">{{{__ "footer.motors.p-z"}}}</a></li>
	        </ul>
		</div>
	    
	    <div class="iab-logo rightColumn">
	        <a href='http://pubads.g.doubleclick.net/gampad/clk?id=25798386&iu=/7162/Gumtree_ZA/zzz_track' target="_blank"><div class="icon-iab-logo"></div></a>
	    </div>
	</div>


	<div class="social-links">{{> (dynamic "socialMediaLinks") }}</div>
	<div class="cpyrt">{{{__ "footer.copyright"}}}</div>
{{/partial}}


