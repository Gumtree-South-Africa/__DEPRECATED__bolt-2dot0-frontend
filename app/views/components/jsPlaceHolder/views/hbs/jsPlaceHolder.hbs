{{#block "marketoTracking"}}
{{!-- Marketo Munchkin Tracking Code ---}}

{{#if model.header.marketo.configMarketo.enabled}}
    {{#if model.header.marketo.configMarketo.munchkinId}}
        <script>
            (function marketoTracking() {
                var wsInfo, configJSON = {};
                var isCookieAnonEnabled = {{#if model.header.marketo.configMarketo.trackAnonUser}}{{model.header.marketo.configMarketo.trackAnonUser}}{{else}}false{{/if}};
                var mkID = "{{model.header.marketo.configMarketo.munchkinId}}";
                var mkCookie = BOLT.UTILS.Cookie.readCookie("_mkto_trk") || "";

                isCookieAnonEnabled = isCookieAnonEnabled ? true: false;
                wsInfo =  {{#if model.header.marketo.configMarketo.wsInfo}}"{{model.header.marketo.configMarketo.wsInfo}}"{{else}}false{{/if}};;
                configJSON = { cookieAnon: isCookieAnonEnabled };
                if (wsInfo) {
                    configJSON["wsInfo"] = wsInfo;
                }

                $.ajax({
                    url: document.location.protocol + '//munchkin.marketo.net/munchkin.js',
                    dataType: 'script',
                    cache: true,
                    async : true,
                    success: function() {
                        if (typeof Munchkin !== "undefined") {
                            Munchkin.init(mkID, configJSON);
                        }
                    }
                });
                $("form :input[name='_mrk_trk']").val(mkCookie);
            })();
        </script>
    {{/if}}
{{/if}}
{{!-- End Marketo Munchkin Tracking Code ---}}

