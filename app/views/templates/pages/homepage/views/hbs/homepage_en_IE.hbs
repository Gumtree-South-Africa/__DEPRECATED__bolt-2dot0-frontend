{{> (base "homepage") }}

{{#if model.header.isIncludeContentForMobile}}
    {{#partial "locationAfterCategory"}}
        {{#block "locationContainer"}}{{/block}}
    {{/partial}}
{{else}}
    {{#partial "locationBeforeCategory"}}
        {{> (dynamic "adCarousel") }}
        {{#block "carousel" }}{{/block}}
    {{/partial}}
    {{#partial "locationAfterCategory"}}
    	{{> (dynamic "locationMapContainer" )}}
        {{#block "locationContainer"}}{{/block}}
    {{/partial}}
{{/if}}