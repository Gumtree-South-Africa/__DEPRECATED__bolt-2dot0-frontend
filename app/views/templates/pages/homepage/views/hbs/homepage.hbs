<!-- THE TITLE IN HOMEPAGE IS: {{title}}

<script src="/js/libraries/handlebars/handlebars.js"></script>
<script src="/js/precompiled/templates.js"></script>
<script src="/js/common/homepage.js"></script>
-->
{{#partial "content"}}
<div style="border: 2px solid green ;overflow: hidden; display: block;">
	<div class="precompiled hello">

	</div>
	<h1>HOME PAGE CONTENTS ==> {{title}} hello {{v1}}
	<span class="hp-live_ads_msg">{{{i18n "home.h1Tag"}}}</span>
	</h1>
	<a href='/message'>MC Page Link</a>
	<hr />

	{{#block "homepg"}}
	 {{#if model.content.topHomePageAdBanner}}
		 {{> hbs/home/AdBanner_top}}
	 {{/if}}
	 
	{{!-- > hbs/widget/carousel --}}
	{{#block "carousel"}}VIEW RESULT{{/block}}
	
	<div class="hp-content hp-others
	{{#if model.socialMediaEnabled}}socialMediaEnabled{{/if}} 
	{{#if model.footer.googleAdSense.enabled}}googleAdSensei{{/if}}
	{{#if model.content.homePageBannerUrl}}homePageBannerImage{{/if}}
	">
	      
			<div class="hp-list-wrapper">
	            {{#with model.content}}
	            <div class="hp-total-ad-count">
	                <span class="hp-live_ads_msg">{{{i18n "home.page.live.ad.count.label" totalLiveAdCount}}}</span>
	            </div>
	            {{/with}}
	
	            <div class="hp-category-wrapper">
	                <h1>{{i18n "home.h1Tag"}}</h1>
	                {{#if model.lastWeekPostedAdCount}}
	                    <p class="lastweekposts">{{{i18n "home.lastweekpostedAD" model.lastWeekPostedAdCount model.lastWeekPostedAdLocal}}}</p>
	                {{/if}}
	
	                {{#block "locationBeforeCategory"}}{{/block}}
	
	                {{#with model.content.categoryFilter}}
	                    {{!-- > hbs/common/CategoryList --}}
	                {{/with}}
	
					<div class="hp-link-wrapper">
						<div class="post-ad-button">
							<a href="{{model.header.postAdPageUrl}}" data-gtm="pc|PostAdBegin" class="primary-button postcommercial">{{i18n "header.postad-long"}}</a>
						</div>
					</div>
	
				</div>
	            
	            {{#block "googleBannerAdsBottom_Container"}}{{/block}}
				<div class="clear"></div>
	            {{#block "locationAfterCategory"}}{{/block}}
	
				<div class="hp-keywords-wrapper">
					{{#if model.content.keywordsModel.topKeywords}}
						<div class="hp-popular-searches-wrapper">
							<h5>{{i18n "home.popular-searches"}}</h5>
							<ul>
							{{#each model.content.keywordsModel.topKeywords}}
								<li data-index="{{@index}}">
									<a href="{{url}}">{{displayName}}</a>
								</li>
							{{/each}}
							</ul>
						</div>
					{{/if}}
		
					{{#if model.content.trendingKeywordsModel.topKeywords}}
						<div class="hp-trending-searches-wrapper">
							<h5>{{i18n "home.trending-searches"}}</h5>
							<ul>
							{{#each model.content.trendingKeywordsModel.topKeywords}}
								<li data-index="{{@index}}">
									<a href="{{url}}">{{displayName}}</a>
								</li>
							{{/each}}
							</ul>
						</div>
					{{/if}}
				</div>
				
				<div class="clear"></div>
		    </div>
	
		
		<input type="hidden" name="NcatId" value="{{model.content.ad.ad.categoryId}}" />
		<input type="hidden" name="NlocId" value="{{model.content.ad.ad.locationId}}" />
	{{/block}}

</div>
{{/partial}}

{{!-- > hbs/Html --}}

