{{#partial "content"}}
	<div>
				<div class="card trending-card">
            <div class="card-title card-title-cp">Popular in <span class="link inline-block underline">Your Neighborhood</span></div>
            {{> (dynamic "tileGrid")}}
            <div class="card-view-more">
                <a class="link" href="/viewMore-TBD">
                    <span class=" underline">View More</span>
                    <span class="no-underline">&nbsp;&gt;</span></a>
            </div>
        </div>

        {{> (dynamic "recentActivity")}}
				{{> (dynamic "modal")}}
        {{> (dynamic "appDownloadSection")}}
        {{> (dynamic "safetyTips") }}
        {{> (dynamic "button")}}
	</div>
{{/partial}}

{{!-- override of header and footer --}}
{{#if this.isNewHP}}
    {{#partial "headerWrapper"}}{{/partial}}
    {{#partial "footerWrapper"}}{{/partial}}
{{/if}}
