            <iframe src="//www.googletagmanager.com/ns.html?id={{model.footer.countryAnalytics.gtmTagId}}"
              height="0" width="0" style="display:none;visibility:hidden">
            </iframe>
        </noscript>
    {{/if}}
     

    <noscript>
        <div class="js-message-error">
            <div>
                <span class="icon-warning-sign"></span>
                <div class="no-js {{model.locale}} message"></div>
            </div>
        </div>
    </noscript>

    {{> Body}}

