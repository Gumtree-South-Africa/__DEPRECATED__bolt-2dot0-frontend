      {{/if}}
    {{/block}}
    <script>
        var Bolt = Bolt || {};
        Bolt.BASEJSURL = "{{model.footer.baseJSUrl}}";
        Bolt.BASECSSURL = "{{model.footer.baseCSSUrl}}";
        Bolt.BASEIMAGEURL = "{{model.footer.baseImageUrl}}";
        Bolt.BRANDNAME = "{{model.footer.brandName}}";
        Bolt.COUNTRY = "{{model.footer.country}}";
        Bolt.DECIMAL = "{{i18n "currency.decimal"}}";
        Bolt.PLACEHOLDER = "{{i18n "currency.placeholder"}}";
        Bolt.LOCALE = "{{model.locale}}";
        Bolt.LOCALIZEAPIROOTURL = "{{model.footer.localizeApiRootUrl}}{{model.locale}}";
    </script>
    {{#block "googleAnayltics"}}
        <input id='ga-account' type='hidden' value='{{account}}' />
        <input id='ga-domain' type='hidden' value='{{domainName}}' />
        <script type="text/javascript">
            var _gaq = _gaq || [];
        {{#if model.footer.countryAnalytics.account}}
            {{#with model.footer.countryAnalytics}}
                _gaq.push(['siteTracker._setAccount', '{{account}}']);
                _gaq.push(['siteTracker._setAllowAnchor', {{allowAnchor}}]);
                _gaq.push(['siteTracker._setDomainName', '{{domainName}}']);
                {{#each ignoredRefs}}
                    _gaq.push(['siteTracker._addIgnoredRef', '{{this}}']);
                {{/each}}
                _gaq.push(['siteTracker._setSessionCookieTimeout', {{sessionCookieTimeout}}]);
                _gaq.push(['siteTracker._setCampaignCookieTimeout', {{campaignCookieTimeout}}]);
                _gaq.push(['siteTracker._setVisitorCookieTimeout', {{visitorCookieTimeout}}]);
                _gaq.push(['siteTracker._trackPageview']);

                (function() {
                    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                    ga.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';
                    //ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
                })();


            {{/with}}
        {{/if}}
        </script>
    {{/block}}

    {{#block "dynamicStyles"}}

    {{/block}}

</head>
<body>
    {{#block "banner_cookie"}}{{/block}}
