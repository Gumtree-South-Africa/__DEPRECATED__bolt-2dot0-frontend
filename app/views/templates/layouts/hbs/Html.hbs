<html data-locale="{{model.locale}}" lang="{{model.header.languageCode}}" class="ie9 lt-ie10 {{model.header.pageType}}"><![endif]-->
<!--[if (gt IE 9)|(gt IEMobile 7)]><!-->
<html data-locale="{{model.locale}}" lang="{{model.header.languageCode}}" xmlns="http://www.w3.org/1999/html" class="{{model.header.pageType}}"><!--<![endif]-->
<head>
    <title>{{#block "title"}}{{model.header.pageTitle}}{{/block}}</title>
    <meta http-equiv='Content-Type' content='text/html; charset=UTF-8'/>
    <meta name="description" content="{{model.header.metaDescription}}"/>
    <meta name="robots" content="{{#if model.header.metaRobots}}{{model.header.metaRobots}}{{else}}index, follow{{/if}}"/>
    <meta name="format-detection" content="telephone=no"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    {{#block "deviceSmartBanner"}}{{/block}}
    {{#block "metaGoogleSearchBox"}}{{/block}}

    {{!-- need to remove it after seo time verify it. --}}
    {{{model.bingMeta}}}

    {{#if model.header.pageUrl}}
        <meta property="og:title" content="{{model.header.pageTitle}}"/>
        <meta property="og:type" content="{{model.header.type}}"/>
        <meta property="og:image" content='{{#block "ogImg"}}{{model.header.logoUrlOpenGraph}}{{/block}}'/>
        <meta property="og:url" content="{{model.header.pageUrl}}"/>
        <meta property="og:site_name" content="{{model.header.brandName}}"/>
        <meta property="og:country-name" content="{{model.header.countryName}}"/>
        <meta property="og:description" content="{{model.header.metaDescription}}"/>
        <meta property="og:locale" content="{{model.header.localeOpenGraph}}"/>
    {{/if}}
    
    {{#with model.header.seoDeeplinkingUrlAndroid}}
       <link rel="alternate" href="{{model.header.seoDeeplinkingUrlAndroid}}"/>
    {{/with}}
    
    {{#each model.header.continerCSS}}
        <link rel="stylesheet" type="text/css" href='{{#block "cssSrc"}}{{this}}{{/block}}'/>

    {{/each}}
    <link rel="publisher" href="{{model.header.publisherUrl}}" />
    <link rel="shortcut icon" type="image/png" href="{{model.header.shortuctIconUrl}}"/>
    <link rel="shortcut icon" type="image/x-icon" href="{{model.header.shortuctIconUrl}}"/>
    <link rel="shortcut icon" type="image/vnd.microsoft.icon" href="{{model.header.shortuctIconUrl}}"/>
    <link rel="apple-touch-icon" href="{{model.header.touchIconIphoneUrl}}"/>
    <link rel="apple-touch-icon" sizes="72x72" href="{{model.header.touchIconIpadUrl}}"/>
    <link rel="apple-touch-icon" sizes="114x114" href="{{model.header.touchIconIphoneRetinaUrl}}"/>
    <link rel="apple-touch-icon" sizes="144x144" href="{{model.header.touchIconIpadRetinaUrl}}"/>


    {{#if model.header.canonical}}
        <link rel="canonical" href="{{model.header.canonical}}"/>
    {{/if}}
    {{#block "meta"}}{{/block}}
    {{#block "metaForPage"}}{{/block}}
       
    {{#block "grumpicon-script"}}

