    {{/block}}
    {{#block "GTMTag"}}
      {{#if model.footer.countryAnalytics.gtmTagId}}

            <script>
            var dataLayer = dataLayer || [];
            dataLayer.push({});(function(l) {
                var dl=l[0];
                dl["p"]={};
                     {{#with model.dataLayer.pageData.pageType}}dl["p"]["t"] = "{{this}}";{{/with}}
                     {{#with model.dataLayer.pageData.platform}}dl["p"]["pl"] = "{{this}}";{{/with}}
                     {{#with model.dataLayer.pageData.version}}dl["p"]["v"] = "{{this}}";{{/with}}
                     {{#with model.dataLayer.pageData.language}}dl["p"]["lng"] ="{{this}}";{{/with}}
                     {{#with model.dataLayer.pageData.viewType}}dl["p"]["vt"] ="{{this}}";{{/with}}
                dl["u"]={};
                dl["u"]["tg"]={};
                     {{#with model.dataLayer.userData.hashedUserId}}dl["u"]["huid"] = "{{this}}";{{/with}}
                     {{#with model.dataLayer.userData.hashedUserEmail}}dl["u"]["hue"] = "{{this}}";{{/with}}
                     {{#with model.dataLayer.userData.hashedAccountId}}dl["u"]["haid"] = "{{this}}";{{/with}}
                     {{#with model.dataLayer.userData.accountType}}dl["u"]["at"] ="{{this}}";{{/with}}
                     {{#with model.dataLayer.userData.loggedIn}}dl["u"]["li"] ="{{this}}";{{/with}} 
                     {{#ifEq model.locale "en_ZA"}}{{#ifEq model.dataLayer.pageData.pageType "PostAdForm"}} dl["u"]["tg"]["ptg"] ="{{model.header.aBTestPostAdUrl}}{{#ifABTestEq "postAd" "A"}}post-log-A{{/ifABTestEq}}{{#ifABTestEq "postAd" "B"}}post-log-B{{/ifABTestEq}}"; {{/ifEq}}{{/ifEq}}
                     {{#ifEq model.locale "en_ZA"}}{{#ifEq model.dataLayer.pageData.pageType "PostAdForm"}} dl["u"]["tg"]["ptg"] ="{{model.header.aBTestPostAdUrl}}{{#ifABTestEq "categoryMobile" "A"}}m-post-cat-A{{/ifABTestEq}}{{#ifABTestEq "categoryMobile" "B"}}m-post-cat-B{{/ifABTestEq}}";{{/ifEq}} {{/ifEq}}
                     {{#ifEq model.locale "en_ZA"}}{{#ifEq model.dataLayer.pageData.pageType "PostAdForm"}} dl["u"]["tg"]["ptg"] ="{{model.header.aBTestPostAdUrl}}{{#ifABTestEq "postAdMobile" "A"}}m-post-att-A{{/ifABTestEq}}{{#ifABTestEq "postAdMobile" "B"}}m-post-att-B{{/ifABTestEq}}";{{/ifEq}} {{/ifEq}}
                    
                dl["c"]={};
                dl["c"]["c"]={};
                     {{#with model.dataLayer.categoryData.current}} dl["c"]["c"]["id"] = "{{this.id}}";{{/with}}
                     {{#with model.dataLayer.categoryData.level0}}dl["c"]["l0"]={};dl["c"]["l0"]["id"]="{{this.id}}";{{/with}}
                     {{#with model.dataLayer.categoryData.level1}}dl["c"]["l1"]={};dl["c"]["l1"]["id"]="{{this.id}}";{{/with}}
                     {{#with model.dataLayer.categoryData.level2}}dl["c"]["l2"]={};dl["c"]["l2"]["id"]="{{this.id}}";{{/with}}
                     {{#with model.dataLayer.categoryData.level3}}dl["c"]["l3"]={};dl["c"]["l3"]["id"]="{{this.id}}";{{/with}}
                     {{#with model.dataLayer.categoryData.level4}}dl["c"]["l4"]={};dl["c"]["l4"]["id"]="{{this.id}}";{{/with}}
                dl["l"]={};
                dl["l"]["c"]={};
                     {{#with model.dataLayer.locationData.current}} dl["l"]["c"]["id"] = "{{this.id}}";{{/with}}
                     {{#with model.dataLayer.locationData.level0}}dl["l"]["l0"]={};dl["l"]["l0"]["id"]="{{this.id}}";{{/with}}
                     {{#with model.dataLayer.locationData.level1}}dl["l"]["l1"]={};dl["l"]["l1"]["id"]="{{this.id}}";{{/with}}
                     {{#with model.dataLayer.locationData.level2}}dl["l"]["l2"]={};dl["l"]["l2"]["id"]="{{this.id}}";{{/with}}
                     {{#with model.dataLayer.locationData.level3}}dl["l"]["l3"]={};dl["l"]["l3"]["id"]="{{this.id}}";{{/with}}
                     {{#with model.dataLayer.locationData.level4}}dl["l"]["l4"]={};dl["l"]["l4"]["id"]="{{this.id}}";{{/with}}

             {{#block "GoogleTagManager"}}{{/block}}

            })(dataLayer);

                    //gtm script
                     (function(w,d,s,l,i){w[l]=w[l]||[];w[l].push(
                             {'gtm.start': new Date().getTime(),event:'gtm.js'}
                     );var f=d.getElementsByTagName(s)[0],
