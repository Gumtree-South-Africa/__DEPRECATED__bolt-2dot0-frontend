{{#partial "headerWrapper"}}
    {{> (dynamic "headerV2")}}
{{/partial}}

{{#partial "content"}}
	<div>
		{{> (dynamic "sectionMenu")}}

        <div class="card trending-card">
            <div class="card-title">Popular in <a class="link inline-block underline" href="/selectLocationOverlay-TBD">Your Neighborhood</a></div>
            {{> (dynamic "tileGrid")}}
            <div class="card-view-more">
                <a class="link" href="/viewMore-TBD">
                    <span class=" underline">View More</span>
                    <span class="no-underline">&nbsp;&gt;</span></a>
            </div>
        </div>

        {{> (dynamic "recentActivity")}}
        {{> (dynamic "appDownloadSection")}}
        {{> (dynamic "safetyTips") }}
        {{> (dynamic "button")}}
	</div>
{{/partial}}

{{#partial "footerWrapper"}}{{/partial}}
