<div id="edit-ad-custom-attributes-form" class="custom-attributes">
    <div>
        {{#each this.customAttributes}}
            <div class="form-field {{#if this.required}}required-field{{/if}}" data-field="{{this.name}}">
                {{#compare this.allowedValueType "===" "LIST"}}
                    {{> (base "editFormSelectBox") this parent=../model}}
                {{/compare}}
                {{#compare this.allowedValueType "===" "DATE"}}
                    {{> (base "editFormDateInput") this parent=../model}}
                {{/compare}}
                {{#compare this.allowedValueType "!==" "LIST"}}
                    {{#compare this.allowedValueType "!==" "DATE"}}
                        {{> (base "editFormTextbox") this parent=../model}}
                    {{/compare}}
                {{/compare}}
            </div>
        {{/each}}
    </div>
</div>

