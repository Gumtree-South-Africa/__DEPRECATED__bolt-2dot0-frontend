<div class="label-wrapper">
	<label for="{{this.name}}">{{this.localizedName}}</label>
</div>
<div class="input-wrapper">
    <input type="{{#if this.allowedValueType}}{{this.allowedValueType}}{{else}}text{{/if}}" name="{{this.name}}" data-validation="{{this.allowedValueType}}" value="{{lookup parent.adResult.attributeValues this.name}}"/>
</div>
