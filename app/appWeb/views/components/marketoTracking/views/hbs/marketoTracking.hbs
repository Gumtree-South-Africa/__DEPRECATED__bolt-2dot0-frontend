{{!-- @MUNCHKIN: Marketo Event Tracking --}}
{{!-- Marketo Munchkin API Associate Lead Function Tracking Call --}}
{{#if this.header.marketo.enabled}}
    {{#if this.header.marketo.munchkinId}}
            function trackEvtMunchkin(mkinTrkJSON, mkinHash) {
                if (typeof Munchkin != "undefined" && typeof mkinTrkJSON != "undefined" && typeof mkinHash != "undefined") {
                    if (typeof mkinTrkJSON !== "object") {
                        mkinTrkJSON = window.atob(mkinTrkJSON);
                        mkinTrkJSON = jQuery.parseJSON(mkinTrkJSON);
                    }
                    Munchkin.munchkinFunction('associateLead', mkinTrkJSON, window.atob(mkinHash));
                }
            }
    {{/if}}
{{/if}}
{{!-- End Marketo Munchkin Tracking --}}