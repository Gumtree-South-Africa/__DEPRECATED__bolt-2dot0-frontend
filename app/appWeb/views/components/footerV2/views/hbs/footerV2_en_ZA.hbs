{{#partial "footer-links-legal"}}
<div class="menu-category"><span>{{i18n "footer.sitemap.legal.legal"}}</span>
  <span class="chevron mobile-only"></span>
  <div class="menu-items">
    <div><a href="{{this.footer.terms}}" data-gtm='pc|NavFooterClick|eventLabel|{{i18n "footer.sitemap.business.terms"}}'>{{i18n "footer.sitemap.business.terms"}}</a></div>
    <div><a href="{{this.footer.privacy}}" data-gtm='pc|NavFooterClick|eventLabel|{{i18n "footer.sitemap.business.privacy"}}'>{{i18n "footer.sitemap.business.privacy"}}</a></div>
    <div><a href="{{this.footer.cookies}}" data-gtm='pc|NavFooterClick|eventLabel|{{i18n "footer.sitemap.business.cookies"}}'>{{i18n "footer.sitemap.business.cookies"}}</a></div>
  </div>
</div>
{{/partial}}

