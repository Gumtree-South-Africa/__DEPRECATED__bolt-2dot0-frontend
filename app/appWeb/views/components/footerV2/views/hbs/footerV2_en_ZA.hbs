    <div><a rel="nofollow" href='' data-gtm='pc|NavFooterClick|eventLabel|{{i18n "footer.sitemap.tips.vivaguides"}}' target="/blank">{{i18n "footer.sitemap.tips.vivaguides"}}</a></div>
    <div><a rel="nofollow" href='' data-gtm='pc|NavFooterClick|eventLabel|{{i18n "footer.sitemap.tips.givefeedback"}}' target="/blank">{{i18n "footer.sitemap.tips.givefeedback"}}</a></div>
