{
	"$schema": "http://json-schema.org/draft-04/schema",
	"type": "object",
	"required": true,
	"properties": {
		"emailAddress": {
			"required": true,
			"pattern": "^(([A-Za-z0-9-]+_+)|([A-Za-z0-9-]+\\-+)|([A-Za-z0-9-]+\\.+)|([A-Za-z0-9-]+\\++))*[A-Za-z0-9-_]+@((\\w+\\-+)|(\\w+\\.))*\\w{1,63}\\.[a-zA-Z]{2,6}$",
			"type": "string",
			"maxLength": 64
		},
		"password": {
			"required": true,
			"type": "string",
			"minLength": 6,
			"maxLength": 25
		}
	}
}
